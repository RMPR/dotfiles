def extract_tokens(phrase):
    phrase_list = re.split("\s+", re.sub("-", " ", phrase))
    phrase_tokenized = []
    for word in phrase_list:
        phrase_tokenized.append(word)
        if phrase_tokenized[-1].isdigit():
            phrase_tokenized[-1] += word.next()
    return phrase_tokenized
