def insertdata(data):
    data_added = False
    i, n = 0, len(listoflist)
    while not data_added and i < n:
        if len(listoflist[i]) < 2:
            listoflist[i].append(data)
            data_added = True
        else:
            i += 1
    if i == n:
        print("\n The list is full, No more elements will be added \n")
while True:
    insertdata(_execute())
    print(listoflist)
