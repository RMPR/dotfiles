def list_numbers_in_words(n):
    ns = str(n)
    single_digits = ["zero", "one", "two", "three",  
                     "four", "five", "six", "seven",  
                     "eight", "nine"]
    return [
        single_digits[int(nsi)]
        for nsi in ns
    ]
