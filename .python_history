def extract_tokens(phrase):
    phrase_list = re.split("\s+", re.sub("-", " ", phrase).strip())
    phrase_tokenized = []
    n = len(phrase_list)
    while i < n:
        phrase_tokenized.append(phrase_list[i])
        if phrase_list[i].isdigit() and i < n-1:
            phrase_tokenized[-1] += " " + phrase_list[i+1]
            i += 1
        i += 1
    return phrase_tokenized
