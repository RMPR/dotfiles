def myreduce(func, lst):
    x = 0
    x1 = x + 1
    result = lst[x]
    while x1 < len(lst):
        applyfunc = func(result, lst[x1]) #This is the line in question
        result = applyfunc
        x += 1
        x1 += 1
    return round(result, 1)
print(myreduce('`complex`', [3, -1, 4.5, 23, 10]))
print(myreduce(complex, [3, -1, 4.5, 23, 10]))
