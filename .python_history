def print_total_scores(text_file):
    with open(text_file) as f:
        lines = f.read().splitlines()
        for line in lines:
            line_splitted = line.split(',')
            name = line_splitted[0]
            total = sum([int(x) for x in line_splitted[1:]])
            print(name + ": " + str(total))
