import os
def copy(file, new_dir):
   file_name=file[1:]
   file_name=file_name.replace('/', "_")
   file_path=os.path.join(new_dir, file_name)
   shutil.copy(file, new_dir)
   os.chdir(os.path.abspath(new_dir))
   os.rename(list(os.path.split(file))[1], file_name)
for file in a:
 copy(file, '.')
